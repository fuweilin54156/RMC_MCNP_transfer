linux
mpiexec -n 200 workspace/RMC -s 12 workspace/inp -r workspace/inp.binary  --status workspace/status
mpiexec -n 1 rmc2ctf
mpiexec -n 177 cobratf
mpiexec -n 1 cobratf_preproc
mpiexec -n 1 ctf2rmc

tianhe2
